                                                    tierBg = 'bg-emerald-500/10';
                                                    tierRing = 'stroke-emerald-500';
                                                    icon = 'fa-bolt';
                                                    label = 'Gold';
                                                    } else if (lead.score >= 50) {
                                                    tierColor = 'text-amber-400';
                                                    tierBg = 'bg-amber-500/10';
                                                    tierRing = 'stroke-amber-500';
                                                    icon = 'fa-star';
                                                    label = 'Silver';
                                                    }

                                                    // SVG calc
                                                    const radius = 18;
                                                    const circumference = 2 * Math.PI * radius;
                                                    const offset = circumference - ((lead.score / 100) * circumference);
                                                    %>
                                                    <div class="flex items-center gap-3">
                                                        <div
                                                            class="relative w-12 h-12 flex items-center justify-center">
                                                            <!-- Background Ring -->
                                                            <svg class="w-full h-full transform -rotate-90">
