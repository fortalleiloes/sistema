                                                </tr>
                                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>

                <script>
                    function switchTab(tabName) {
                        const activeClasses = ['shadow-sm', 'bg-[var(--macos-bg-secondary)]', 'text-[var(--macos-text-primary)]'];
                        const inactiveClasses = ['text-[var(--macos-text-tertiary)]', 'hover:text-[var(--macos-text-primary)]'];

                        const btnLeads = document.getElementById('tab-leads');
                        const btnBlacklist = document.getElementById('tab-blacklist');

                        // Reset to inactive state base
                        btnLeads.classList.remove(...activeClasses);
                        btnLeads.classList.add(...inactiveClasses);

                        btnBlacklist.classList.remove(...activeClasses);
                        btnBlacklist.classList.add(...inactiveClasses);

                        // Set active
                        const activeBtn = tabName === 'leads' ? btnLeads : btnBlacklist;
                        activeBtn.classList.remove(...inactiveClasses);
                        activeBtn.classList.add(...activeClasses);

                        // Toggle Panels using classes
                        const panelLeads = document.getElementById('panel-leads');
                        const panelBlacklist = document.getElementById('panel-blacklist');

                        if (tabName === 'leads') {
                            panelLeads.classList.remove('hidden');
                            panelBlacklist.classList.add('hidden');
                        } else {
                            panelLeads.classList.add('hidden');
                            panelBlacklist.classList.remove('hidden');
                        }
                    }

                    // Auto-init
                    document.addEventListener('DOMContentLoaded', () => {
                        switchTab('leads');
                    });
                </script>

                </div>
            </main>

            <script>
                document.addEventListener('DOMContentLoaded', function () {
                    const stateFilter = document.getElementById('stateFilter');
                    const rows = document.querySelectorAll('.lead-row');

                    console.log('âœ… Filtro de Estados Carregado. Total de linhas:', rows.length);

                    // 1. Extrair estados Ãºnicos
                    const states = new Set();
                    rows.forEach(row => {
                        const stateRaw = row.getAttribute('data-state');
                        if (stateRaw && stateRaw.trim() !== '' && stateRaw !== 'UF') {
                            states.add(stateRaw.trim().toUpperCase());
                        }
                    });

                    console.log('ðŸ“ Estados encontrados:', Array.from(states));

                    // 2. Popular o select
                    const sortedStates = Array.from(states).sort();
                    sortedStates.forEach(state => {
                        const option = document.createElement('option');
                        option.value = state;
                        option.textContent = state;
                        stateFilter.appendChild(option);
                    });

                    // 3. LÃ³gica de filtro
                    stateFilter.addEventListener('change', function () {
                        const selectedState = this.value;
                        console.log('ðŸ” Filtrando por:', selectedState);

                        rows.forEach(row => {
                            const rowState = (row.getAttribute('data-state') || '').trim().toUpperCase();

                            if (!selectedState || rowState === selectedState) {
                                row.style.display = '';
                            } else {
                                row.style.display = 'none';
                            }
                        });
                    });
                    // 4. DetecÃ§Ã£o de Duplicatas (Retroativa + Nova)
                    const trackingMap = {};

                    // Contar ocorrÃªncias
                    rows.forEach(row => {
                        const raw = row.getAttribute('data-tracking') || '|';
                        const [whatsapp, fingerprint] = raw.split('|');

                        // Chaves possiveis
                        if (whatsapp) {
                            trackingMap['wa:' + whatsapp] = (trackingMap['wa:' + whatsapp] || 0) + 1;
                        }
                        if (fingerprint && fingerprint.length > 5) {
                            trackingMap['fp:' + fingerprint] = (trackingMap['fp:' + fingerprint] || 0) + 1;
                        }
                    });

                    // Aplicar badges
                    rows.forEach(row => {
                        const raw = row.getAttribute('data-tracking') || '|';
                        const [whatsapp, fingerprint] = raw.split('|');
                        let isDup = false;
                        let count = 0;
                        let reason = '';

                        if (whatsapp && trackingMap['wa:' + whatsapp] > 1) {
                            isDup = true;
                            count = trackingMap['wa:' + whatsapp];
                            reason = 'Telefone repetido';
                        } else if (fingerprint && fingerprint.length > 5 && trackingMap['fp:' + fingerprint] > 1) {
                            isDup = true;
                            count = trackingMap['fp:' + fingerprint];
                            reason = 'Mesmo dispositivo';
                        }

                        if (isDup) {
                            // Encontrar a cÃ©lula do nome (2Âª coluna) e adicionar badge
                            const nameCell = row.querySelectorAll('td')[1];
                            const badge = document.createElement('div');
                            badge.className = 'inline-flex items-center px-1.5 py-0.5 rounded text-[10px] font-bold bg-red-500/10 text-red-500 border border-red-500/20 mt-1';
                            badge.innerHTML = `<i class="fa-solid fa-clone mr-1"></i> ${count}x (${reason})`;

                            // Adicionar logo abaixo do nome
                            const nameEl = nameCell.querySelector('.font-semibold');
                            if (nameEl) nameEl.insertAdjacentElement('afterend', badge);
                        }
                    });
                });
            </script>
</body>

